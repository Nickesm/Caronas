        <activity
            android:name=".OferecerActivity"
            android:label="@string/title_activity_oferecer" >
        </activity>
        <activity
            android:name=".SobreActivity"
            android:label="@string/title_activity_sobre" >
        </activity>
    </application>

</manifest>
